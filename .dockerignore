# Ignore Python bytecode
*.pyc
*.pyo
__pycache__/

# Ignore temporary files and directories
*.swp
*.swo
*~

# Ignore any hidden files or directories (e.g., .git, .vscode)
.*
!.dockerignore

# Ignore output directory if it exists locally
output/

# Ignore GPX files (they will be mounted during runtime)
gpx_files/

# Ignore Dockerfile and requirements.txt if not needed in the image
Dockerfile
requirements.txt

# Explicitly ignore the .git directory
.git/
